   "source": [
    "import numpy as np\n",
    "\n",
    "\n",
    "a = np.array([1, 2, 3, 4, 5])\n",
    "\n",
    "mask = a > 3    # filter expression\n",
    "temp=a[mask]\n",
    "print(temp)  # [4, 5]\n",
    "\n",
    "\n",
    "# create array by passing expression\n",
    "b= np.array([x for x in range(5)])\n",
    "print(b)\n",
    "\n",
    "\n"
   ]
  }
